<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:flypay="clr-namespace:FlyPay.Pages;assembly=FlyPay"
            xmlns:flypaymenu="clr-namespace:FlyPay.Menu;assembly=FlyPay"
            x:Class="FlyPay.FlyoutMainPage">
    
    <FlyoutPage.Flyout>
        <flypaymenu:FlyoutMenuPage x:Name="flyoutPage" />
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <flypay:AccountOverviewPage />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>